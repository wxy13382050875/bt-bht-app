@charset "UTF-8";
/*统计标题样式*/
/*box*/
/*统计页面头部 chart-header-bg*/
.dialog {
  background: #ffffff;
  border-radius: 30rpx 30rpx 0px 0px;
}
.dialog .dialog-header {
    height: 80rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    line-height: 80rpx;
    -webkit-box-pack: center;
    -webkit-justify-content: center;
            justify-content: center;
    -webkit-box-align: center;
    -webkit-align-items: center;
            align-items: center;
    font-size: 30rpx;
    color: #333333;
}
.dialog .dialog-content {
    margin-left: 20rpx;
    margin-right: 20rpx;
}
.dialog .dialog-content .dialog-item {
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
      height: 80rpx;
      line-height: 80rpx;
      border-bottom: 1rpx solid #a7a7a7;
}
.dialog .dialog-content .dialog-item .name {
        width: 200rpx;
        font-size: 28rpx;
        color: #333333;
}
.dialog .dialog-content .dialog-item .content {
        font-size: 28rpx;
        color: #9a9a9a;
}
.dialog .dialog-finish {
    height: 117rpx;
    width: 100%;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    -webkit-align-items: center;
            align-items: center;
}
.dialog .dialog-finish .finish-btn {
      height: 87rpx;
      width: 690rpx;
      background: #ff3333;
      opacity: 1;
      border-radius: 41px 41px 41px 41px;
      color: #ffffff;
      font-size: 30rpx;
}

