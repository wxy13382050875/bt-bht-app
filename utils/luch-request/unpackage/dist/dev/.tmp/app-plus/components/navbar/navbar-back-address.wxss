@charset "UTF-8";
/*统计标题样式*/
/*box*/
/*统计页面头部 chart-header-bg*/
.navbar-back-container {
  position: fixed;
  top: 0;
  right: 0;
  left: 0;
  z-index: 1993;
  text-align: center;
  background: -webkit-linear-gradient(left, #e42332, #e40011, #b10613);
  background: linear-gradient(90deg, #e42332, #e40011, #b10613);
}
.navbar-back-wrapper {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  padding: 0 18rpx;
}
.navbar-back-wrapper .nav-back-wrapper {
    position: relative;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    -webkit-align-items: center;
            align-items: center;
    width: 100%;
    height: 100%;
    -webkit-box-pack: justify;
    -webkit-justify-content: space-between;
            justify-content: space-between;
}
.navbar-back-wrapper .nav-back-wrapper .nav-back {
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
      width: 32px;
      height: 100%;
      -webkit-box-align: center;
      -webkit-align-items: center;
              align-items: center;
      color: #fff;
      font-size: 34rpx;
      z-index: 22;
}
.navbar-back-wrapper .nav-back-wrapper .nav-back .icon {
        font-size: 34rpx;
}
.navbar-back-wrapper .nav-back-wrapper .title {
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
      -webkit-box-pack: justify;
      -webkit-justify-content: space-between;
              justify-content: space-between;
      position: absolute;
      top: 0;
      right: 0;
      bottom: 0;
      left: 55rpx;
      display: flex;
      -webkit-box-align: center;
      -webkit-align-items: center;
              align-items: center;
      height: 100%;
      font-size: 30rpx;
      color: #fff;
}

