@charset "UTF-8";
/*统计标题样式*/
/*box*/
/*统计页面头部 chart-header-bg*/
.address-list .swipe-action .swipe-action-itme .address-cell {
  width: 100%;
  margin-top: 10rpx;
  margin-bottom: 10rpx;
  background: #ffffff;
  height: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.address-list .swipe-action .swipe-action-itme .address-cell .a-img {
    margin-left: 20rpx;
    width: 65rpx;
    height: 69rpx;
}
.address-list .swipe-action .swipe-action-itme .address-cell .a-img image {
      margin: 0 auto;
      /*水平居中*/
      width: 65rpx;
      height: 79rpx;
      position: absolute;
      z-index: 80;
}
.address-list .swipe-action .swipe-action-itme .address-cell .a-img .a-surname {
      width: 65rpx;
      height: 79rpx;
      line-height: 79rpx;
      position: absolute;
      z-index: 100;
      color: #ffffff;
      font-size: 32rpx;
      text-align: center;
      vertical-align: middle;
}
.address-list .swipe-action .swipe-action-itme .address-cell .a-address-info {
    margin-left: 30rpx;
    margin-right: 20rpx;
    width: 100%;
}
.address-list .swipe-action .swipe-action-itme .address-cell .a-address-info .a-oneline {
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
      height: 80rpx;
      line-height: 80rpx;
}
.address-list .swipe-action .swipe-action-itme .address-cell .a-address-info .a-oneline .a-name {
        color: #333333;
        font-size: 32rpx;
}
.address-list .swipe-action .swipe-action-itme .address-cell .a-address-info .a-oneline .a-phone {
        color: #999999;
        font-size: 26rpx;
        margin-left: 13rpx;
}
.address-list .swipe-action .swipe-action-itme .address-cell .a-address-info .a-twoline {
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
      -webkit-box-pack: justify;
      -webkit-justify-content: space-between;
              justify-content: space-between;
}
.address-list .swipe-action .swipe-action-itme .address-cell .a-address-info .a-twoline .a-default {
        width: 60rpx;
        color: #ff3333;
        font-size: 26rpx;
        margin-right: 20rpx;
}
.address-list .swipe-action .swipe-action-itme .address-cell .a-address-info .a-twoline .a-address {
        color: #333333;
        font-size: 26rpx;
}
.address-list .swipe-action .swipe-action-itme .address-cell .a-address-info .a-twoline .a-edit {
        width: 127rpx;
        text-align: center;
        border-left: 1rpx solid #b2b4b3;
        margin-bottom: 10rpx;
}

