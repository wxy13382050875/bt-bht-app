/*统计页面头部 chart-header-bg*/
.navbar-universal-container {
  position: fixed;
  top: 0;
  right: 0;
  left: 0;
  z-index: 993;
  text-align: center;
  background: -webkit-linear-gradient(left, #e42332, #e40011, #b10613);
  background: linear-gradient(90deg, #e42332, #e40011, #b10613);
}
.navbar-universal-wrapper {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  padding: 0 18rpx;
}
.navbar-universal-wrapper .nav-universal-wrapper {
    position: relative;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    -webkit-align-items: center;
            align-items: center;
    width: 100%;
    height: 100%;
    -webkit-box-pack: justify;
    -webkit-justify-content: space-between;
            justify-content: space-between;
}
.navbar-universal-wrapper .nav-universal-wrapper .nav-back {
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
      height: 100%;
      -webkit-box-align: center;
      -webkit-align-items: center;
              align-items: center;
      color: #fff;
      font-size: 34rpx;
      z-index: 22;
}
.navbar-universal-wrapper .nav-universal-wrapper .nav-back .icon {
        font-size: 34rpx;
}
