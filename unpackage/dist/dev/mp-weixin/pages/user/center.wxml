<view class="app-content"><center-nav-bar vue-id="1" title="个人中心" bind:__l="__l"></center-nav-bar><bht-layout-container vue-id="2" bg-color="#fff" bind:__l="__l" vue-slots="{{['default']}}"><view class="my-center-bg"></view><view class="my-center-main"><view class="my-center-box"><view class="left"><view class="avator"></view><view class="info"><label class="name">{{userInfo.name}}</label><text class="desc">{{userInfo.mobile}}</text></view></view><view class="right"><router-link vue-id="{{('3')+','+('2')}}" to="{name: 'edit-profile'}" bind:__l="__l" vue-slots="{{['default']}}"><view class="iconfont aca-youjiantou"></view></router-link></view></view><view class="aca-list"><block wx:if="{{userInfo.type==='001'}}"><router-link vue-id="{{('4')+','+('2')}}" to="{name: 'bill-management'}" bind:__l="__l" vue-slots="{{['default']}}"><view class="aca-list-item"><view class="icon"><image src="/static/icon/invoice_list_icon.png"></image></view><view class="content"><view class="title">开票管理</view><view class="arrow-right"><view class="iconfont aca-youjiantou"></view></view></view></view></router-link></block><router-link vue-id="{{('5')+','+('2')}}" to="{name: 'profile'}" bind:__l="__l" vue-slots="{{['default']}}"><view class="aca-list-item"><view class="icon"><image src="/static/icon/invoice_userinfo_icon.png"></image></view><view class="content"><view class="title">我的信息</view><view class="arrow-right"><view class="iconfont aca-youjiantou"></view></view></view></view></router-link></view><view data-event-opts="{{[['tap',[['loginOut',['$event']]]]]}}" class="login-out-btn" bindtap="__e">退出</view></view></bht-layout-container></view>