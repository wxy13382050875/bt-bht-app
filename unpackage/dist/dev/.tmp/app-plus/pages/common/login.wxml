<view><aca-nav-bar vue-id="1" level="{{2}}" title="登录" bind:__l="__l"></aca-nav-bar><bht-layout-container vue-id="2" bgColor="#fff" bind:__l="__l" vue-slots="{{['default']}}"><view class="login-wrapper" style="{{'bottom:'+(bottom+'rpx')+';'}}"><view class="login-top"><view class="logo-box"><image class="app-logo" src="/static/app_logo_d.png"></image></view><image class="app-logo-2" src="/static/app_logo_login1.png"></image></view><view class="app-login"><view class="aca-form-input"><view class="aca-input-icon"><view class="iconfont aca-shouji"></view></view><input class="aca-input" type="number" name="input" placeholder="请输入手机号" data-event-opts="{{[['input',[['__set_model',['$0','account','$event',[]],['loginData']]]]]}}" value="{{loginData.account}}" bindinput="__e"/></view><view class="aca-form-input"><view class="aca-input-icon"><view class="iconfont aca-mima"></view></view><input class="aca-input" type="password" name="input" placeholder="请输入密码" data-event-opts="{{[['focus',[['inputFocus',['$event']]]],['blur',[['inputBlur',['$event']]]],['input',[['__set_model',['$0','password','$event',[]],['loginData']]]]]}}" value="{{loginData.password}}" bindfocus="__e" bindblur="__e" bindinput="__e"/></view><button class="app-btn" hover-class data-event-opts="{{[['tap',[['handleLogin',['$event']]]]]}}" bindtap="__e">登录</button><view class="login-footer"><router-link class="item" vue-id="{{('3')+','+('2')}}" to="{name: 'forget'}" bind:__l="__l" vue-slots="{{['default']}}">忘记密码？</router-link><router-link class="item" vue-id="{{('4')+','+('2')}}" to="{name: 'reg'}" bind:__l="__l" vue-slots="{{['default']}}">注册</router-link></view></view></view></bht-layout-container></view>