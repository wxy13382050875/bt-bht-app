.bht-layout-container {
  position: relative;
  -webkit-overflow-scrolling: touch;
  overflow: hidden;
}
.bht-layout-container::-webkit-scrollbar {
    display: none;
    width: 0;
    height: 0;
    color: transparent;
}
.bht-layout-container .space-h {
    width: 100%;
    height: 100rpx;
    line-height: 100rpx;
}

